            "application/json": {
              "schema": {
                "type": "object",
                "additionalProperties": true,
                "required": ["plan_action", "client", "plan"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Plan created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {},
