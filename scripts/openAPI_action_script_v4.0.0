          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["record_id", "client_id", "status"],
                "properties": {
                  "record_id": { "type": "string" },
                  "client_id": { "type": "string" },
                  "status": { "type": "string" },
                  "expected_duration": { "type": "string", "nullable": true },
                  "date_resolved": { "type": "string", "nullable": true },
                  "notes": { "type": "string", "nullable": true }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Updated",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {},
